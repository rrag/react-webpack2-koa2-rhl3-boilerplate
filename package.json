    "html-webpack-plugin": "^2.22.0",
    "jest": "^15.1.1",
    "koa": "^2.0.0-alpha.7",
    "koa-static": "^3.0.0",
    "koa-webpack-middleware": "^1.0.1",
    "progress-bar-webpack-plugin": "^1.9.0",
    "react-addons-test-utils": "^15.3.1",
    "react-test-renderer": "^15.3.1",
    "rimraf": "^2.5.4",
    "webpack": "^2.1.0-beta.22",
    "webpack-config-utils": "^2.1.0"
  }
