  "private": true,
  "description": "react-webpack2-koa2-rhl3-boilerplate",
  "scripts": {
    "watch": "node scripts/watch.js",
    "serve": "node scripts/serve.js",
    "clean": "rimraf build && rimraf coverage && rimraf npm-debug.log",
    "build": "npm run clean && npm run build:dev && npm run build:prod",
    "build:prod": "webpack --config config/webpack.base.config.js --env prod",
    "build:dev": "webpack --config config/webpack.base.config.js --env dev",
    "lint": "eslint --ext .js,.jsx src",
    "test": "jest --coverage",
    "validate": "npm run lint && npm run test",
    "watch:test": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rrag/react-webpack2-koa2-rhl3-boilerplate"
